// Program for bitwise
#include <stdio.h>

int main()
{
    int a,b;
    scanf("%d %d",&a,&b);
    printf("\n bitwise &:%d",a&b);
    printf("\n bitwise |:%d",a|b);
    printf("\n bitwise ^:%d",a^b);
    printf("\n bitwise ~:%d",~a);
    printf("\n bitwise <<:%d",a<<2);
    printf("\n bitwise >>:%d",a>>2);
}    
    
    
